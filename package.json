{
  "name": "better-open-editors",
  "displayName": "Better Open Editors",
  "description": "Groups open editors by package folders or RegEx patterns.",
  "keywords": [
    "editors",
    "tabs",
    "groups",
    "packages",
    "monorepo"
  ],
  "version": "1.12.0",
  "preview": false,
  "license": "MIT",
  "icon": "meta/icon.png",
  "repository": "https://github.com/McSodbrenner/vscode-better-open-editors",
  "engines": {
    "vscode": "^1.69.0"
  },
  "publisher": "McSodbrenner",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "betterOpenEditors.openFileOfCurrentPackage",
        "key": "ctrl+oem_1",
        "mac": "cmd+[BracketLeft]"
      }
    ],
    "configuration": {
      "title": "Better Open Editors",
      "properties": {
        "betterOpenEditors.InsertSpaceForLastSlug": {
          "order": 1,
          "type": "boolean",
          "default": false,
          "description": "Insert space before the last slug to improve the readability."
        },
        "betterOpenEditors.InsertSpacesAroundSlashes": {
          "order": 2,
          "type": "boolean",
          "default": false,
          "description": "Insert spaces around all path slashes to improve the readability."
        },
        "betterOpenEditors.ShowWorkspaceIcon": {
          "order": 3,
          "type": "boolean",
          "default": false,
          "description": "Shows a Workspace Icon in front of workspace folders."
        },
        "betterOpenEditors.ShowPackageIcon": {
          "order": 4,
          "type": "boolean",
          "default": false,
          "description": "Shows a Package Icon in front of packages."
        },
        "betterOpenEditors.PackagePatterns": {
          "order": 5,
          "type": "string",
          "editPresentation": "multilineText",
          "default": "**/components/*\n**/modules/*",
          "description": "One glob expression per line which creates a package folder if a parent path of a file matches it, e.g. '**/components/*'. Useful in workspace dependent settings."
        },
        "betterOpenEditors.ShowPackageInfo": {
          "order": 6,
          "type": "boolean",
          "default": true,
          "description": "Shows name and version if there is a package file."
        },
        "betterOpenEditors.HidePackagePath": {
          "order": 7,
          "type": "boolean",
          "default": false,
          "description": "Hides the path of a package if a package name is available."
        },
        "betterOpenEditors.HideFilePath": {
          "order": 8,
          "type": "boolean",
          "default": false,
          "description": "Hides the path of a file."
        },
        "betterOpenEditors.SkipWorkspacesIfNotNeeded": {
          "order": 9,
          "type": "boolean",
          "default": true,
          "description": "Skips the workspace tree level if you have only one folder opened. But it could be useful to disable this setting if you want to see the package info for the workspace folder."
        },
        "betterOpenEditors.TabNumbering": {
          "order": 10,
          "type": "string",
          "enum": [
            "none",
            "left",
            "right"
          ],
          "enumDescriptions": [
            "No numbering.",
            "Tab index to the left of the filename.",
            "Tab index to the right of the filename."
          ],
          "default": "none",
          "description": "Adds numbering to the tabs."
        }
      }
    },
    "views": {
      "explorer": [
        {
          "id": "betterOpenEditors",
          "name": "Better Open Editors"
        }
      ]
    },
    "commands": [
      {
        "command": "betterOpenEditors.openFileOfCurrentPackage",
        "title": "Open file of current package",
        "icon": "$(files)"
      },
      {
        "command": "betterOpenEditors.refreshTree",
        "title": "Refresh tree",
        "icon": "$(refresh)"
      },
      {
        "command": "betterOpenEditors.openExtensionSettings",
        "title": "Open settings",
        "icon": "$(settings-gear)"
      },
      {
        "command": "betterOpenEditors.collapseAll",
        "title": "Collapse all",
        "icon": "$(collapse-all)"
      },
      {
        "command": "betterOpenEditors.saveAll",
        "title": "Save all",
        "icon": "$(save-all)"
      },
      {
        "command": "betterOpenEditors.showTab",
        "title": "Show tab"
      },
      {
        "command": "betterOpenEditors.closeTab",
        "title": "Close",
        "icon": "$(close)"
      },
      {
        "command": "betterOpenEditors.closeFolder",
        "title": "Close all tabs of package",
        "icon": "$(close)"
      },
      {
        "command": "betterOpenEditors.openInIntegratedTerminal",
        "title": "Open in integrated terminal",
        "icon": "$(terminal)"
      },
      {
        "command": "betterOpenEditors.copyPath",
        "title": "Copy path"
      },
      {
        "command": "betterOpenEditors.copyRelativePath",
        "title": "Copy relative path"
      },
      {
        "command": "betterOpenEditors.revealFileInOS",
        "title": "Reveal in file explorer"
      },
      {
        "command": "betterOpenEditors.pinTab",
        "title": "Pin tab"
      },
      {
        "command": "betterOpenEditors.unpinTab",
        "title": "Unpin tab"
      },
      {
        "command": "betterOpenEditors.openPackageFile",
        "title": "Open package file"
      },
      {
        "command": "betterOpenEditors.showUnknownFileInfo",
        "title": "Show UnknownFile info"
      },
      { "command": "betterOpenEditors.showTab1", "title": "Show Tab 1" },
      { "command": "betterOpenEditors.showTab2", "title": "Show Tab 2" },
      { "command": "betterOpenEditors.showTab3", "title": "Show Tab 3" },
      { "command": "betterOpenEditors.showTab4", "title": "Show Tab 4" },
      { "command": "betterOpenEditors.showTab5", "title": "Show Tab 5" },
      { "command": "betterOpenEditors.showTab6", "title": "Show Tab 6" },
      { "command": "betterOpenEditors.showTab7", "title": "Show Tab 7" },
      { "command": "betterOpenEditors.showTab8", "title": "Show Tab 8" },
      { "command": "betterOpenEditors.showTab9", "title": "Show Tab 9" },
      { "command": "betterOpenEditors.showTaba", "title": "Show Tab a" },
      { "command": "betterOpenEditors.showTabb", "title": "Show Tab b" },
      { "command": "betterOpenEditors.showTabc", "title": "Show Tab c" },
      { "command": "betterOpenEditors.showTabd", "title": "Show Tab d" },
      { "command": "betterOpenEditors.showTabe", "title": "Show Tab e" },
      { "command": "betterOpenEditors.showTabf", "title": "Show Tab f" },
      { "command": "betterOpenEditors.showTabg", "title": "Show Tab g" },
      { "command": "betterOpenEditors.showTabh", "title": "Show Tab h" },
      { "command": "betterOpenEditors.showTabi", "title": "Show Tab i" },
      { "command": "betterOpenEditors.showTabj", "title": "Show Tab j" },
      { "command": "betterOpenEditors.showTabk", "title": "Show Tab k" },
      { "command": "betterOpenEditors.showTabl", "title": "Show Tab l" },
      { "command": "betterOpenEditors.showTabm", "title": "Show Tab m" },
      { "command": "betterOpenEditors.showTabn", "title": "Show Tab n" },
      { "command": "betterOpenEditors.showTabo", "title": "Show Tab o" },
      { "command": "betterOpenEditors.showTabp", "title": "Show Tab p" },
      { "command": "betterOpenEditors.showTabq", "title": "Show Tab q" },
      { "command": "betterOpenEditors.showTabr", "title": "Show Tab r" },
      { "command": "betterOpenEditors.showTabs", "title": "Show Tab s" },
      { "command": "betterOpenEditors.showTabt", "title": "Show Tab t" },
      { "command": "betterOpenEditors.showTabu", "title": "Show Tab u" },
      { "command": "betterOpenEditors.showTabv", "title": "Show Tab v" },
      { "command": "betterOpenEditors.showTabw", "title": "Show Tab w" },
      { "command": "betterOpenEditors.showTabx", "title": "Show Tab x" },
      { "command": "betterOpenEditors.showTaby", "title": "Show Tab y" },
      { "command": "betterOpenEditors.showTabz", "title": "Show Tab z" }
    ],
    "menus": {
      "view/title": [
        {
          "command": "betterOpenEditors.saveAll",
          "when": "view == betterOpenEditors",
          "group": "navigation"
        },
        {
          "command": "betterOpenEditors.collapseAll",
          "when": "view == betterOpenEditors",
          "group": "navigation"
        },
        {
          "command": "betterOpenEditors.refreshTree",
          "when": "view == betterOpenEditors",
          "group": "hidden"
        },
        {
          "command": "betterOpenEditors.openExtensionSettings",
          "when": "view == betterOpenEditors",
          "group": "hidden"
        }
      ],
      "view/item/context": [
        {
          "command": "betterOpenEditors.closeTab",
          "group": "inline",
          "when": "view == betterOpenEditors && viewItem =~ /<file>/"
        },
        {
          "command": "betterOpenEditors.openInIntegratedTerminal",
          "group": "inline",
          "when": "view == betterOpenEditors && viewItem =~ /<workspace>/"
        },
        {
          "command": "betterOpenEditors.openInIntegratedTerminal",
          "group": "inline",
          "when": "view == betterOpenEditors && viewItem =~ /<folder>/"
        },
        {
          "command": "betterOpenEditors.closeFolder",
          "group": "inline",
          "when": "view == betterOpenEditors && viewItem =~ /<folder>/"
        },
        {
          "command": "betterOpenEditors.copyPath",
          "group": "1-copypath",
          "when": "view == betterOpenEditors && viewItem =~ /<file>/"
        },
        {
          "command": "betterOpenEditors.copyPath",
          "group": "1-copypath",
          "when": "view == betterOpenEditors && viewItem =~ /<folder>/"
        },
        {
          "command": "betterOpenEditors.copyPath",
          "group": "1-copypath",
          "when": "view == betterOpenEditors && viewItem =~ /<workspace>/"
        },
        {
          "command": "betterOpenEditors.copyRelativePath",
          "group": "1-copypath",
          "when": "view == betterOpenEditors && viewItem =~ /<file>/"
        },
        {
          "command": "betterOpenEditors.copyRelativePath",
          "group": "1-copypath",
          "when": "view == betterOpenEditors && viewItem =~ /<folder>/"
        },
        {
          "command": "betterOpenEditors.revealFileInOS",
          "group": "2-special",
          "when": "view == betterOpenEditors && viewItem =~ /<file>/"
        },
        {
          "command": "betterOpenEditors.revealFileInOS",
          "group": "2-special",
          "when": "view == betterOpenEditors && viewItem =~ /<folder>/"
        },
        {
          "command": "betterOpenEditors.revealFileInOS",
          "group": "2-special",
          "when": "view == betterOpenEditors && viewItem =~ /<workspace>/"
        },
        {
          "command": "betterOpenEditors.openPackageFile",
          "group": "2-special",
          "when": "view == betterOpenEditors && viewItem =~ /<workspace>/ && viewItem =~ /<realpackage>/"
        },
        {
          "command": "betterOpenEditors.openPackageFile",
          "group": "2-special",
          "when": "view == betterOpenEditors && viewItem =~ /<folder>/ && viewItem =~ /<realpackage>/"
        },
        {
          "command": "betterOpenEditors.openInIntegratedTerminal",
          "group": "2-special",
          "when": "view == betterOpenEditors && viewItem =~ /<workspace>/"
        },
        {
          "command": "betterOpenEditors.openInIntegratedTerminal",
          "group": "2-special",
          "when": "view == betterOpenEditors && viewItem =~ /<folder>/"
        },
        {
          "command": "betterOpenEditors.pinTab",
          "group": "3-specialPin",
          "when": "view == betterOpenEditors && viewItem =~ /<file>/ && viewItem =~ /<current>/"
        },
        {
          "command": "betterOpenEditors.unpinTab",
          "group": "3-specialPin",
          "when": "view == betterOpenEditors && viewItem =~ /<file>/ && viewItem =~ /<current>/ && viewItem =~ /<pinned>/"
        },
        {
          "command": "betterOpenEditors.closeTab",
          "group": "4-close",
          "when": "view == betterOpenEditors && (viewItem =~ /<file>/ || viewItem =~ /<unknownfile>/)"
        },
        {
          "command": "betterOpenEditors.closeFolder",
          "group": "4-close",
          "when": "view == betterOpenEditors && viewItem =~ /<folder>/"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "eslint": "^8.23.0",
    "eslint-plugin-import": "^2.26.0"
  },
  "dependencies": {
    "lodash.orderby": "^4.6.0",
    "minimatch": "^5.1.0",
    "sort-paths": "^1.1.1"
  }
}
